{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"propertySuggester": false
	},
	"rules": {
		"no-underscore-dangle": "off",
		"no-jquery/variable-pattern": "off",
		"jsdoc/no-undefined-types": [
			"off",
			{
				"definedTypes": [
					"wikibase.Entity"
				]
			}
		],
		"prefer-const": "warn"
	}
}
