{
	"name": "propertysuggester/property-suggester",
	"type": "mediawiki-extension",
	"description": "Extension for Wikibase to provide usefull suggestions for new properties",
	"keywords": [
		"propertysuggester",
		"wikibase",
		"wikidata"
	],
	"homepage": "http://wikiba.se",
	"license": "GPL-2.0+",
	"require": {
		"php": ">=5.5.9",
		"wikibase/data-model": "~6.1|~7.0",
		"wikibase/data-model-services": "~1.1|~2.0|~3.0"
	},
	"require-dev": {
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"phpunit/phpunit": "~4.8",
		"satooshi/php-coveralls": "dev-master",
		"wikibase/wikibase-codesniffer": "^0.2.0",
		"wikimedia/testing-access-wrapper": "~1.0",
		"mediawiki/minus-x": "0.2.0"
	},
	"autoload": {
		"psr-4": {
			"PropertySuggester\\": "src/PropertySuggester/"
		},
		"files": [],
		"classmap": [
			"PropertySuggesterHooks.php",
			"maintenance/UpdateTable.php"
		]
	},
	"scripts": {
		"fix": [
			"phpcbf",
			"minus-x fix ."
		],
		"test": [
			"parallel-lint . --exclude vendor",
			"phpcs -p -s",
			"minus-x check ."
		]
	}
}
