{
	"name": "propertysuggester/property-suggester",
	"type": "mediawiki-extension",
	"description": "Extension for Wikibase to provide useful suggestions for new properties",
	"keywords": [
		"propertysuggester",
		"wikibase",
		"wikidata"
	],
	"homepage": "http://wikiba.se",
	"license": "GPL-2.0-or-later",
	"require": {
		"wikibase/data-model": "^9.0.1",
		"wikibase/data-model-services": "~4.0|~5.0"
	},
	"require-dev": {
		"mediawiki/mediawiki-codesniffer": "33.0.0",
		"mediawiki/mediawiki-phan-config": "0.10.4",
		"mediawiki/minus-x": "1.1.0",
		"php-coveralls/php-coveralls": "^2.0.0",
		"php-parallel-lint/php-console-highlighter": "0.5.0",
		"php-parallel-lint/php-parallel-lint": "1.2.0",
		"wikimedia/testing-access-wrapper": "~1.0"
	},
	"scripts": {
		"fix": [
			"minus-x fix .",
			"phpcbf"
		],
		"test": [
			"parallel-lint . --exclude vendor --exclude node_modules",
			"phpcs -p -s",
			"minus-x check ."
		]
	}
}
