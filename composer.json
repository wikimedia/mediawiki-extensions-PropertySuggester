{
	"name": "propertysuggester/property-suggester",
	"type": "mediawiki-extension",
	"description": "Extension for Wikibase to provide usefull suggestions for new properties",
	"keywords": [
		"propertysuggester",
		"wikibase",
		"wikidata"
	],
	"homepage": "http://wikiba.se",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": ">=5.5.9",
		"wikibase/data-model": "~6.1|~7.0",
		"wikibase/data-model-services": "~1.1|~2.0|~3.0"
	},
	"require-dev": {
		"mediawiki/minus-x": "0.3.1",
		"php-parallel-lint/php-console-highlighter": "0.3.2",
		"php-parallel-lint/php-parallel-lint": "1.0.0",
		"satooshi/php-coveralls": "^2.0",
		"wikibase/wikibase-codesniffer": "0.5.1",
		"wikimedia/testing-access-wrapper": "~1.0"
	},
	"scripts": {
		"fix": [
			"minus-x fix .",
			"phpcbf"
		],
		"test": [
			"parallel-lint . --exclude vendor --exclude node_modules",
			"phpcs -p -s",
			"minus-x check ."
		]
	}
}
